#!/bin/bash

cd ../../bin
export CLASSPATH=$CLASSPATH:server/:client/

#Lancement servers
java tests.ServerMainTree > output &
echo "[OK] Lancement du serveur"

toKill=$!
sleep 0.1

#Generation de l'arbre
java tests.CreateTree 
echo "[OK] Cr√©ation de l'arbre"


kill $toKill
echo "[OK] Kill du serveur"
